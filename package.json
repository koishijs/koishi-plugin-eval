{
  "name": "@root/eval",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "packageManager": "yarn@1.22.19",
  "license": "MIT",
  "scripts": {
    "esbuild": "yakumo esbuild",
    "build": "yakumo tsc && yakumo esbuild",
    "bump": "yakumo version",
    "dep": "yakumo upgrade",
    "pub": "yakumo publish",
    "test": "yakumo mocha -r esbuild-register -r yml-register --experimental-vm-modules",
    "test:text": "shx rm -rf coverage && c8 -r text yarn test",
    "test:json": "shx rm -rf coverage && c8 -r json yarn test",
    "test:html": "shx rm -rf coverage && c8 -r html yarn test"
  },
  "devDependencies": {
    "@sinonjs/fake-timers": "^6.0.1",
    "@types/chai": "^4.3.3",
    "@types/chai-as-promised": "^7.1.5",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.7.18",
    "@types/sinonjs__fake-timers": "^6.0.4",
    "c8": "^7.12.0",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "chai-shape": "^1.0.0",
    "esbuild": "^0.14.54",
    "esbuild-register": "^3.3.3",
    "jest-mock": "^28.1.3",
    "mocha": "^9.2.2",
    "shx": "^0.3.4",
    "typescript": "^4.8.3",
    "yml-register": "^1.1.0",
    "yakumo": "^0.3.6",
    "yakumo-esbuild": "^0.3.17",
    "yakumo-esbuild-yaml": "^0.3.1",
    "yakumo-mocha": "^0.3.0",
    "yakumo-publish": "^0.3.0",
    "yakumo-publish-sync": "^0.3.1",
    "yakumo-tsc": "^0.3.5",
    "yakumo-upgrade": "^0.3.2",
    "yakumo-version": "^0.3.1"
  }
}
